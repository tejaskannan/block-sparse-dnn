{
    "name": "sparse_rnn",
    "batch_size": 64,
    "learning_rate": 0.001,
    "learning_rate_decay": 0.99,
    "gradient_clip": 1,
    "num_epochs": 25,
    "patience": 25,
    "optimizer": "adam",
    "dropout_keep_rate": 1.0,
    "state_size": 128,
    "rnn_layers": 2,
    "hidden_units": [256, 256],
    "hidden_activation": "leaky_relu",
    "should_layer_normalize": true,
    "should_normalize_inputs": false,
    "sparsity": 1.5,
    "sparse_format": "coo",
    "prune_fraction": 0.1,
    "dataset_type": "memory"
}

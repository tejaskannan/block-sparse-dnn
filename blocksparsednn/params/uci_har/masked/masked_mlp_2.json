{
    "name": "masked_mlp",
    "batch_size": 16,
    "learning_rate": 0.001,
    "learning_rate_decay": 0.99,
    "gradient_clip": 1,
    "num_epochs": 50,
    "patience": 50,
    "hidden_units": [512, 512],
    "hidden_activation": "leaky_relu",
    "optimizer": "adam",
    "dropout_keep_rate": 1.0,
    "sparsity": 0.025,
    "sparsity_loss_weight": 250.0,
    "warmup": 3,
    "should_layer_normalize": true,
    "should_normalize_inputs": true,
    "dataset_type": "memory"
}

{
    "name": "masked_cnn",
    "batch_size": 128,
    "learning_rate": 0.001,
    "learning_rate_decay": 0.99,
    "gradient_clip": 1,
    "num_epochs": 50,
    "patience": 50,
    "filter_sizes": [3, 3],
    "filter_strides": [1, 1],
    "pool_strides": [2, 2],
    "out_channels": [32, 64],
    "conv_pool_mode": "max",
    "conv_activation": "relu",
    "hidden_units": [256, 256],
    "hidden_activation": "relu",
    "optimizer": "adam",
    "dropout_keep_rate": 1.0,
    "sparsity": 0.025,
    "sparsity_loss_weight": 250.0,
    "should_layer_normalize": true,
    "should_normalize_inputs": false,
    "dataset_type": "memory"
}
